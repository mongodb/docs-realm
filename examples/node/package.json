  "description": "Source for generated js/typescript code samples in the Realm Node.js client guide",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:js": "NODE_OPTIONS=--experimental-vm-modules jest --selectProjects JavaScript --runInBand --detectOpenHandles --forceExit && npm run delete-realm-files",
    "test:ts": "jest --selectProjects TypeScript --runInBand --detectOpenHandles --forceExit && npm run delete-realm-files",
    "lint": "eslint Examples/*",
    "prettier": "prettier --write Examples/*",
    "delete-realm-files": "rm -rf *realm.lock *.realm *realm.note *realm.management mongodb-realm *.realm.fresh.lock realm-files myrealm"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@jest/globals": "^29.5.0",
    "@types/bson": "^4.0.2",
    "@types/jest": "^29.4.0",
    "babel-jest": "^29.4.3",
    "bson": "^4.7.2",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "random-email": "^1.0.3",
    "realm": "^12.0.0-alpha.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "nock": "^13.1.3"
  }
