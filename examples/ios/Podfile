platform :ios, '15.0'

target 'RealmExamples' do
  use_frameworks!
  pod 'SwiftLint'
  pod 'Realm', '=10.18.0'
  pod 'RealmSwift', :git => 'https://github.com/realm/realm-cocoa.git', :branch => 'py/projection'
  pod 'GoogleSignIn'
  pod 'FBSDKLoginKit', '=8.1.0'
end

target 'QuickStartSwiftUI' do
  use_frameworks!

  pod 'SwiftLint'
  pod 'RealmSwift', :git => 'https://github.com/realm/realm-cocoa.git', :branch => 'py/projection'
end
